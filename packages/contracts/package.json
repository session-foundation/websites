{
  "name": "@session/contracts",
  "version": "0.0.0",
  "exports": {
    ".": "./index.ts",
    "./abis": "./abis/index.ts",
    "./chains": "./chains.ts",
    "./hooks/*": "./hooks/*.tsx"
  },
  "scripts": {
    "generate": "tsx ./scripts/generate-abis.ts",
    "generate:debug": "tsx ./scripts/generate-abis.ts --debug",
    "check-types": "tsc --noEmit",
