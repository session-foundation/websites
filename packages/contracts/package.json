    "@tanstack/react-query": "5.61.5",
    "@session/wallet": "workspace:*",
    "viem": "2.21.54",
    "wagmi": "2.13.5"
  },
  "devDependencies": {
    "@session/typescript-config": "workspace:*",
    "@session/testing": "workspace:*",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "react": "19.0.0",
    "tsx": "^4.10.5",
    "yarn": "^1.22.22"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10",
    "yarn": "use pnpm",
    "npm": "use pnpm"
  }
